{
    "name": "mikebirch/cakephp-sponge-blog",
    "description": "Blog plugin for CakePHP 3",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "CakephpSpongeBlog\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CakephpSpongeBlog\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
